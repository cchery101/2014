<div class="news-video">
  <h2 class="news-video__title">
    <a class="news-video__title__link" href="https://www.youtube.com/watch?v={{this.id}}">{{this.title}}</a>
  </h2>
  <p class="news-video__description">{{this.description}}</p>
  <div class="news-video__embed-wrapper">
    <iframe class="news-video__embed-wrapper__embed" width="560" height="315" src="//www.youtube-nocookie.com/embed/{{this.id}}?rel=0" frameborder="0" allowfullscreen title="{{this.title}}"></iframe>
  </div>
</div>
